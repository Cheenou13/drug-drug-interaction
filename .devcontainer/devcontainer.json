{
    "name" : "JM-project",
    "image": "maven:latest",
    "mounts": [
        "source=/var/run/docker.sock,target=/var/run/docker.sock,type=bind"
    ],
    "customizations": {
        "vscode": {
            "settings": {
                "terminal.integrated.defaultProfile.linux": "bash"
            },
            "extensions": [
                "vscjava.vscode-java-debug",
                "ms-vscode-remote.remote-containers",
                "vscjava.vscode-java-pack"
            ]
        }
    },
    "forwardPorts": [8080]
}